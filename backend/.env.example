# OpenAI API Key for GPT-4o analysis
OPENAI_API_KEY=sk-your-openai-api-key-here

# Database URL (SQLite for local dev, PostgreSQL for production)
# Local development (default):
DATABASE_URL=sqlite:///./funnel_analyzer.db
# Production (Railway/PostgreSQL):
# DATABASE_URL=postgresql+asyncpg://user:pass@host:5432/dbname

# JWT secret for authentication
JWT_SECRET=replace-with-long-random-string-min-32-chars

# Environment
ENVIRONMENT=development

# Frontend URL for CORS
FRONTURL=http://localhost:3001

# Server port
PORT=3000
