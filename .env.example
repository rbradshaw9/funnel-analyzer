# Copy this file to `.env` and fill in the values before running the project locally.

# OpenAI API key used for generating embeddings and AI-powered features.
OPENAI_API_KEY=sk-your-openai-api-key

# Database connection string used by the backend service.
DATABASE_URL=postgresql://user:password@host:5432/funnel_analyzer

# Secret used to sign and verify JSON Web Tokens for authentication.
JWT_SECRET=replace-with-a-long-random-string

# Deployment tokens used by the GitHub Actions workflow for automation.
VERCEL_TOKEN=
RAILWAY_TOKEN=
